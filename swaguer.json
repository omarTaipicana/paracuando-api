{
  "openapi": "3.0.3",
  "info": {
    "title": "paracuando_api - OpenAPI 3.0",
    "description": "API DE PUBLICACIONES",
    "termsOfService": "http://swagger.io/terms/",
    "contact": {
      "name": "Fernando - Angel",
      "email": "fernandoparadoja@gmail.com"
    },
    "license": {
      "name": "Apache 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    },
    "version": "1.0.11"
  },
  "externalDocs": {
    "description": "Find out more about Swagger",
    "url": "http://swagger.io"
  },
  "servers": [
    {
      "url": "http://localhost:8001/api/v1",
      "description": "development server"
    }
  ],
  "tags": [
    {
      "name": "auth",
      "description": "Auth login"
    },
    {
      "name": "users",
      "description": "user management"
    }
  ],
  "paths": {
    "/auth/sign-up": {
      "post": {
        "tags": [
          "auth"
        ],
        "summary": "Sign Up user",
        "description": "Sign Up user",
        "operationId": "Sign Up user",
        "requestBody": {
          "description": "User information to register",
          "content": {
            "application/x-www-form-urlencoded": {
              "schema": {
                "$ref": "#/components/schemas/UserSingUp"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "authenticated user"
          }
        }
      }
    },
    "/auth/login": {
      "post": {
        "tags": [
          "auth"
        ],
        "summary": "Authenticate user",
        "description": "Authenticate user",
        "operationId": "Authenticate user",
        "requestBody": {
          "description": "user info to be filled",
          "content": {
            "application/x-www-form-urlencoded": {
              "schema": {
                "$ref": "#/components/schemas/UserLogin"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "authenticated user"
          }
        }
      }
    },
    "/auth/me": {
      "get": {
        "tags": [
          "auth"
        ],
        "summary": "Authenticate user",
        "description": "Authenticate user",
        "operationId": "verify",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "authenticated user"
          },
          "401": {
            "$ref": "#/components/responses/UnauthorizedError"
          }
        }
      }
    },
    "/auth/forget-password": {
      "post": {
        "tags": [
          "auth"
        ],
        "summary": "Set Token",
        "description": "The user sets a token in the DB so that he can change his password.",
        "operationId": "Set Token",
        "requestBody": {
          "description": "user info to be filled",
          "content": {
            "application/x-www-form-urlencoded": {
              "schema": {
                "$ref": "#/components/schemas/UserForgetPassword"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "authenticated user"
          }
        }
      }
    },
    "/auth/change-password/{token}": {
      "post": {
        "tags": [
          "auth"
        ],
        "summary": "Change Password",
        "description": "The token is sent along with the password for the password change to be made",
        "operationId": "Change Password",
        "parameters": [
          {
            "name": "token",
            "in": "path",
            "description": "Token User",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "description": "user info to be filled",
          "required": true,
          "content": {
            "application/x-www-form-urlencoded": {
              "schema": {
                "$ref": "#/components/schemas/UserResetPassword"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "authenticated user"
          }
        }
      }
    },
    "/users": {
      "get": {
        "tags": [
          "users"
        ],
        "summary": "Users filter",
        "description": "Users can be filtered",
        "operationId": "Users filter",
        "parameters": [
          {
            "name": "page",
            "in": "query",
            "description": "Paginacion que se mostrara",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "size",
            "in": "query",
            "description": "Registrot a devolver",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "first_name",
            "in": "query",
            "description": "Se podrafiltrar por este campo",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "last_name",
            "in": "query",
            "description": "Se podrafiltrar por este campo",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "email",
            "in": "query",
            "description": "Se podrafiltrar por este campo",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "username",
            "in": "query",
            "description": "Se podrafiltrar por este campo",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "email_verified",
            "in": "query",
            "description": "Se podrafiltrar por este campo",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "country_id",
            "in": "query",
            "description": "Se podrafiltrar por este campo",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "code_phone",
            "in": "query",
            "description": "Se podrafiltrar por este campo",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "phone",
            "in": "query",
            "description": "Se podrafiltrar por este campo",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "created_at",
            "in": "query",
            "description": "Se podrafiltrar por este campo",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "vista paginada"
          }
        }
      }
    },
    "/users/{userID}": {
      "get": {
        "tags": [
          "users"
        ],
        "summary": "Find user by id",
        "description": "The public information of the user will be seen, if the user is authenticated, complete fields will be shown",
        "operationId": "User Id",
        "parameters": [
          {
            "name": "userID",
            "in": "path",
            "description": "Id of the user to return",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "succesfull operation"
          }
        }
      },
      "put": {
        "tags": [
          "users"
        ],
        "summary": "edit user by id",
        "description": "User can edit their fields",
        "operationId": "Edit User Id",
        "parameters": [
          {
            "name": "userID",
            "in": "path",
            "description": "Id of the user to return",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "description": "user info to be filled",
          "content": {
            "application/x-www-form-urlencoded": {
              "schema": {
                "$ref": "#/components/schemas/UserUpdate"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "succesfull operation"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "UserSingUp": {
        "required": [
          "first_name",
          "last_name",
          "email",
          "password"
        ],
        "type": "object",
        "properties": {
          "first_name": {
            "type": "string",
            "description": "user first name"
          },
          "last_name": {
            "type": "string",
            "description": "user last name"
          },
          "email": {
            "type": "string",
            "format": "email",
            "description": "email"
          },
          "password": {
            "type": "string",
            "format": "password",
            "description": "password"
          }
        }
      },
      "UserUpdate": {
        "type": "object",
        "properties": {
          "first_name": {
            "type": "string",
            "description": "user first name"
          },
          "last_name": {
            "type": "string",
            "description": "user last name"
          },
          "code_phone": {
            "type": "string",
            "description": "user identity"
          },
          "interestes": {
            "type": "string",
            "example": "1,2,3,4"
          }
        }
      },
      "UserLogin": {
        "required": [
          "email",
          "password"
        ],
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "format": "email",
            "description": "user identity"
          },
          "password": {
            "type": "string",
            "format": "password",
            "description": "password"
          }
        }
      },
      "UserForgetPassword": {
        "required": [
          "email"
        ],
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "format": "email",
            "description": "user identity"
          }
        }
      },
      "UserResetPassword": {
        "required": [
          "password"
        ],
        "properties": {
          "password": {
            "type": "string",
            "format": "password",
            "description": "password"
          }
        }
      },
      "PublicationTypeUpdate": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "publication name"
          },
          "description": {
            "type": "string",
            "description": "description of publication"
          }
        }
      },
      "AddPublication": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "publication name"
          },
          "description": {
            "type": "string",
            "description": "description of publication"
          },
          "content": {
            "type": "string",
            "description": "content of publication"
          },
          "reference_link": {
            "type": "string",
            "description": "reference link of publication"
          },
          "publication_type_id": {
            "type": "string",
            "description": "id of publication"
          },
          "tags": {
            "type": "string",
            "description": "tags of publication",
            "example": "1,2,3,4"
          }
        }
      },
      "AddTag": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "tag name"
          },
          "description": {
            "type": "string",
            "description": "description of tag"
          }
        }
      },
      "TagUpdate": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "tag name"
          },
          "description": {
            "type": "string",
            "description": "description of tag"
          }
        }
      }
    },
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    },
    "responses": {
      "UnauthorizedError": {
        "description": "Access token is missing or invalid"
      }
    }
  },
  "security": [
    {
      "bearerAuth": []
    }
  ]
}